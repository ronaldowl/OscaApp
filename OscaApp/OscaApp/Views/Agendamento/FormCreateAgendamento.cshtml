                                            <span class="input-group-addon"><i class="fa fa-asterisk fa-2x"></i></span>
                                            <input id="osc_nome" asp-for="@Model.agendamento.codigo" placeholder="Codigo" class="form-control" type="text" disabled="disabled">
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label" style="color:red">Cliente *</label>
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <span class="fa fa-user-o fa-2x"></span>
                                        </span>
                                        <input id="osc_clienteIdName" asp-for="@Model.cliente.idName" type="text" class="form-control" required="required" disabled="disabled">
                                        <input id="osc_clienteId" asp-for="@Model.cliente.id" type="hidden" class="form-control" />
                                        <span class="input-group-btn ">
                                            <button type="button" class="btn btn-info"  >
                                                <span class="fa fa-search"></span>
                                            </button>
                                        </span>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label>Associar com</label>
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <span class="fa fa-list-ul fa-2x"></span>
                                        </span> &nbsp;
                                        <input id="osc_tipoReferencia" asp-for="@Model.agendamento.tipoReferencia" class="form-control" type="hidden"     value="@Model.agendamento.tipoReferencia" disabled="disabled">                                        
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Data agendada</label>
                                    <div class="inputGroupContainer">
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-calendar fa-2x"></i></span>
                                            <input id="osc_dataInicio" asp-for="@Model.agendamento.dataAgendada" class="form-control" type="text" value="" required="required">
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label>Hora Inicio</label>
                                    <div class="inputGroupContainer">
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-calendar fa-2x"></i></span>
