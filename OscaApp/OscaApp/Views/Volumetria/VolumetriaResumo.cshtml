@model OscaFramework.Models.Volumetria;

@{
    Layout = "_LayoutFrame";
}
<div class="card" style="align-items:flex-start">
    <div class="card-header bg-primary">
        <table>
            <tr>
                <td><i class="fa fa-2x fa-tachometer iconPanel" aria-hidden="true"></i></td>
                <td><h5>Volumetria</h5></td>
            </tr>
        </table>
    </div>
    <div class="card-body">
        <ol style="font-size:12px;font-weight:bold;list-style-type:circle;font-size:12px">
            <li>Clientes: @Model.qtdCliente</li>
            <li>Contatos: @Model.qtdContato</li>
            <li>Produtos: @Model.qtdProduto</li>
        </ol>
    </div>
</div>