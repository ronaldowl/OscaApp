@model X.PagedList.PagedList<OscaFramework.Models.Servico>
@using X.PagedList.Mvc;
@using X.PagedList;
@using X.PagedList.Mvc.Core; @*import this so we get our HTML Helper*@
@{
    Layout = "_LayoutFrame";
}


<table class="table table-hover   table-bordered table-sm">
    <thead class="thead-light">
        <tr>
            <th></th>
            <th>Código</th>
            <th>Servico</th>
            <th>Forma de Venda</th>
            <th>Valor</th>

        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr  style="cursor:pointer;" id="@item.id" onclick="ConfigLookup('@item.id.ToString()','@item.nomeServico.ToString()', '@item.valor.ToString("N2")') " data data-dismiss="modal">
            <td><i class="fa fa-shield"></i></td>
            <td>@item.codigo.ToString()</td>
            <td>@item.nomeServico</td>
            <td>@item.formaVendaServico</td>
            <td>@item.valor.ToString("N2")</td>
        </tr>
    }
</table>

