@model OscaApp.Data.ContextPage
<div class="container-fluid">
    <div class="row">
        <div class="col-md-8 ">
            <h3><i class="fa fa-smile-o fa-1x"></i> @Model.nomeUsuario seja bem vindo ao OSCA!</h3>           
        </div>
    </div>
    <br />
    <br />
    
    <div class="row">
        <div class="col-md-8">
            <div class="row">
                <div class="col-md-6">                  
                   
                    <iframe allowtransparency="true" scrolling="no" src="http://www.horariodebrasilia.org/" width="220" height="180" frameborder="0"></iframe>
                </div>
                <div class="col-md-6">
 
                </div>     
            </div>
        </div>

        <!-- Módulos float right -->
        <div class="col-md-4">           
                <iframe allowtransparency="true" scrolling="no" src="http://www.tempoagora.com.br/ta-widgets?cidades=SaoPaulo-SP&amp;tipo=vertical" width="160" height="260" frameborder="0"></iframe>
                
                        
        </div>

    </div>
</div>
<br /> <br /> <br /> <br /> <br /> <br />
<div class="row">
    <div style="background-color:darkgrey; width: 100%; height:20px"></div>
    <div class="col-sm-3">
        <div class="card m-2">
            <div class="card-body">
                <i class="fa fa-2x fa-cog iconPanel" aria-hidden="true"></i>
                <a asp-controller="Paineis" asp-action="PainelOperacional"><p style="font-size:19.8px; margin:0;">Painel Operacional</p></a>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="card m-2">
            <div class="card-body">
                <i class="fa fa-2x fa-money iconPanel" aria-hidden="true"></i>
                <a asp-controller="Paineis" asp-action="PainelFinanceiro"><h5>Painel Financeiro</h5></a>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="card m-2">
            <div class="card-body">
                <i class="fa fa-2x fa-life-ring iconPanel" aria-hidden="true"></i>
                <a asp-controller="Paineis" asp-action="PainelVendas"><h5>Painel Vendas</h5></a>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="card m-2">
            <div class="card-body">
                <i class="fa fa-2x fa-life-ring iconPanel" aria-hidden="true"></i>
                <a asp-controller="Paineis" asp-action="PainelServico"><h5>Painel Serviços</h5></a>
            </div>
        </div>
    </div>
</div>





 
