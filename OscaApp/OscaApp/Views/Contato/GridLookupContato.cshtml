@model X.PagedList.PagedList<OscaFramework.Models.Contato>
@using X.PagedList.Mvc;
@using X.PagedList;
@using X.PagedList.Mvc.Core; @*import this so we get our HTML Helper*@
@{
    Layout = "_LayoutFrame";
}

<table class="table table-hover table-striped table-bordered table-sm">
    <thead>
        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th>Telefone</th>
        </tr>
    </thead>
    @foreach (var item in Model)
            {
        <tr id="@item.id" value="@item.nome" onclick="ConfigLookup('@item.id.ToString()','@item.nome.ToString()') " data-dismiss="modal">
            <td>@item.nome</td>
            <td>@item.email</td>
            <td>@item.telefone</td>
        </tr>
    }
</table>



